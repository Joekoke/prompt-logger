{
    "manifest_version": 3,
    "name": "Prompt Logger",
    "version": "1.0",
    "description": "Log prompts and seeds from any webpage without copy-paste",
    "permissions": [
          "storage",
          "activeTab",
          "scripting"
    ],
    "action": {
          "default_popup": "popup.html",
          "default_title": "Prompt Logger"
    },
    "icons": {
          "16": "images/icon-16.png",
          "48": "images/icon-48.png",
          "128": "images/icon-128.png"
    }
}
